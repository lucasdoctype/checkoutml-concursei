{
  "name": "concursei-webhook-payment",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/main/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node -r ./dist/otel.js dist/main/index.js",
    "start:all": "concurrently -k \"npm run start\" \"npm run start:worker\"",
    "test": "vitest run",
    "republish:failed": "tsx src/main/republish-failed.ts",
    "worker": "tsx src/main/worker.ts",
    "start:worker": "node -r ./dist/otel.js dist/main/worker.js"
  },
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.67.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.210.0",
    "@opentelemetry/resources": "^2.4.0",
    "@opentelemetry/sdk-node": "^0.210.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@supabase/supabase-js": "^2.39.6",
    "amqplib": "^0.10.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.16.0",
    "concurrently": "^9.1.2",
    "tsx": "^4.15.7",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
